{
    "functions": {
        "api/**/*.js": {
        "maxDuration": 300
        }
    },
    "build": {
        "env": {
        "canvas_version": "2.11.2"
        }
    },
    "headers": [
        {
        "source": "/(.*)",
        "headers": [
            { "key": "Permissions-Policy", "value": "interest-cohort=()" }
        ]
        }
    ],
    "rewrites": [
        {
        "source": "/((?!api/|.*\\..*).*)",
        "destination": "/index.html"
        }
    ]
}